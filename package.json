{
  "name": "kiwi-hackathon",
  "version": "1.0.0",
  "description": "Kiwi.com Hackathon",
  "main": "index.js",
  "engines": {
    "node": "6.10.0"
  },
  "files": [
    "main.js"
  ],
  "dependencies": {
    "claudia-api-builder": "^2.0.0",
    "denodeify": "^1.2.1",
    "superb": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "claudia": "^2.14.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.4"
  },
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets es2015,stage-2",
    "build": "babel index.js -d dist --presets es2015,stage-2",
    "prettier": "prettier --single-quote --trailing-comma all --write '**/*.js'",
    "create": "claudia create --name travelhackers-api --region eu-central-1 --api-module main --profile=Kiwi",
    "deploy": "claudia update"
  }
}